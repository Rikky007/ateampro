<?xml version="1.0" encoding="UTF-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">
	 <!-- title of the playlist -->
    <title>Example Title</title>

    <!-- name of the author -->
    <creator>Example Creator</creator>

    <!-- homepage of the author -->
    <info>http://yourdomain.com</info>
	
    <trackList>
		<track>
			<!-- URI of resource to be rendered -->
			<location>http://yourdomain.com/audio1.mp4</location>
            <location>http://yourdomain.com/audio1.mp3</location>
			<location>http://yourdomain.com/audio1.oga</location>

			<!-- Human-readable name of the entity (author, authors, group, company, etc) that authored the resource -->
            <creator>Creator 1</creator>

            <!-- Human-readable name of the collection from which the resource comes -->
            <album>Album 1</album>

            <!-- Human-readable name of the track that authored the resource -->
            <title>Title 1</title>

            <!-- A human-readable comment on the track -->
            <annotation></annotation>

            <!-- The time to render a resource, in milliseconds -->
            <duration>1234567890</duration>

            <!-- URI of an image to display -->
            <image>http://yourdomain.com/audio1.jpg</image>

			<!-- URI of a place where this resource can be bought or more info can be found -->
			<info>http://albumdownload.com/</info>
			
			<!-- Integer with value greater than zero giving the ordinal position of the media on the xspf:album -->
			<trackNum>1</trackNum>
			
			<!-- The meta element allows metadata fields to be added to xspf:track elements -->
			<meta rel="http://creativecommons.org/licenses/by-sa/3.0/">CC BY-SA 3.0</meta>
        </track>
		
